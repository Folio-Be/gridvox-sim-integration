{
    "name": "@gridvox/ams2-track-extractor",
    "version": "1.0.0",
    "description": "Extract and convert AMS2 race tracks to glTF 2.0 using PCarsTools and Blender",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "build": "tsc",
        "extract": "tsx scripts/extract-track-v2.ts",
        "extract-old": "tsx scripts/extract-track.ts",
        "optimize": "tsx scripts/optimize-gltf.ts",
        "validate": "tsx scripts/validate-track.ts",
        "verify-setup": "tsx scripts/verify-setup.ts",
        "help": "tsx scripts/extract-track-v2.ts --help"
    },
    "keywords": [
        "ams2",
        "automobilista-2",
        "pcarstools",
        "track-extraction",
        "gltf",
        "three.js",
        "blender",
        "visualization",
        "telemetry"
    ],
    "author": "GridVox",
    "license": "MIT",
    "dependencies": {
        "three": "^0.181.0",
        "@gltf-transform/core": "^4.0.0",
        "@gltf-transform/extensions": "^4.0.0",
        "@gltf-transform/functions": "^4.0.0",
        "commander": "^12.0.0",
        "chalk": "^5.3.0",
        "ora": "^8.0.1"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "@types/three": "^0.181.0",
        "typescript": "^5.3.3",
        "tsx": "^4.7.0",
        "@gltf-transform/cli": "^4.0.0"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/GridVox/gridvox-sim-integration.git",
        "directory": "ams2/ams2-track-extractor"
    }
}